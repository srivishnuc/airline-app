{
 "env": {
  "browser": true,
  "es2021": true,
  "amd": true,
  "node": true
 },
 "extends": [
  "plugin:react/recommended",
  "google",
  "eslint:recommended",
  "plugin:react/recommended",
  "plugin:prettier/recommended"
 ],
 "overrides": [],
 "parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
 },
 "plugins": ["react"],
 "rules": {
  "prettier/prettier": [
   "error",
   {
    "endOfLine": "auto"
   }
  ]
 }
}
